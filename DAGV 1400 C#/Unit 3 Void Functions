using System;
					
public class Program
{
	public void Main()
	{
		Console.WriteLine("Welcome");
		DoMath(10, 10, 5);
		DoMath(20, 15, 5);
		DoMath(30, 20, 5);
		
	}
	
	public void DoMath (int value, int value2, int value3) {
		var number = value - value2 + value3;
		Console.WriteLine(number);
	}
}